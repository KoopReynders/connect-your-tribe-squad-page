{% render 'partials/head.liquid', title: 'Squad page' %}

<h1>Squad page</h1>

<h2>Students</h2>
<ul>
  {% for person in persons %}
    <li><a href="/student/{{ person.id }}">{{ person.name }}</a> 
    {% comment %} - Squad {{ person.squads.squad_id }}</li> {% endcomment %}
  {% endfor %}
</ul>

{% render 'partials/foot.liquid', squads: squads %}